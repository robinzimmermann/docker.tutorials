<?xml version="1.0" encoding="UTF-8" ?>
<gateway-config xmlns="http://xmlns.kaazing.org/2016/06/gateway">


  <service>

    <name>DMZ Echo Proxy</name>

    <accept>http://example.com:80/</accept>
    <connect>http://example.com:80/</connect>

    <type>http.proxy</type>

    <connect-options>
      <!-- Comment in the following for forwards connectivity -->
      <!-- <http.transport>tcp://internal.example.net:80</http.transport> -->

      <!-- Comment in the following for reverse connectivity -->
      <http.transport>socks://dmz.example.net:1080</http.transport>
      <socks.mode>reverse</socks.mode>
      <socks.transport>wsn://dmz.example.net:1080/enterprise-shield</socks.transport>
    </connect-options>

    <cross-site-constraint>
      <allow-origin>*</allow-origin>
    </cross-site-constraint>

  </service>


</gateway-config>
